{
  "name": "swipeclean",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start -c",
    "ios:16": "./scripts/launch-iphone16.sh",
    "ios:16:metro": "RCT_METRO_PORT=8802 expo start --port 8802",
    "ios:16:run": "RCT_METRO_PORT=8802 expo run:ios --device '783931F3-3F74-44AB-861A-359EEC5D008F'",
    "ios:16pro:guinote": "./scripts/launch-iphone16pro-guinote.sh",
    "ios:both": "./scripts/launch-both-simulators.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release --cleanup",
    "e2e:clean": "detox clean-framework-cache && detox build-framework-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "^4.5.7",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-haptics": "~14.1.4",
    "expo-image": "^2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-localization": "~16.1.6",
    "expo-media-library": "~17.1.7",
    "expo-router": "~5.1.4",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react-native": "^0.539.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "tailwindcss": "^3.4.17",
    "zustand": "^5.0.7",
    "expo-clipboard": "~7.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^19.1.1",
    "@react-native/eslint-config": "^0.80.2",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "detox": "^20.40.2",
    "eslint": "^8.57.1",
    "jest-circus": "^30.0.5",
    "jest-expo": "^53.0.9",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
